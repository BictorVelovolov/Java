package com.example.demo2;
import java.util.Scanner;

public class HelloApplication
{
    public static void main(String[]args){
        Scanner in = new Scanner(System.in);
        int ch, k, nch;
        ch = in.nextInt();
        k = 0;
        nch = ch;
        while (ch % 5 != 0 && ch > 5)
        {
            k = k + 1;
            ch = in.nextInt();
            if (ch > 10)
            {
                nch = nch + ch;
            }

        }
        System.out.print(nch);
    }
}
