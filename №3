package com.example.demo2;
import java.util.Scanner;

public class HelloApplication
{
    public static void main(String[]args)
    {
        Scanner in = new Scanner(System.in);
        int n, nch;
        nch = 0;
        n = in.nextInt();
        while (n > 9 && n < 100){
            nch = nch + n % 10 + n / 10;
            n = in.nextInt();
        }
        System.out.print(nch);
    }
}
